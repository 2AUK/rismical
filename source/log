gfortran -O3 -fomit-frame-pointer -ffixed-form -fallow-argument-mismatch -c rismiofile.f -o rismiofile.o 
gfortran -O3 -fomit-frame-pointer -ffixed-form -fallow-argument-mismatch -c setup1dvx.f -o setup1dvx.o 
gfortran -O3 -fomit-frame-pointer -ffixed-form -fallow-argument-mismatch -c misc.f -o misc.o 
gfortran -O3 -fomit-frame-pointer -ffixed-form -fallow-argument-mismatch -c read3duvdata.f -o read3duvdata.o 
gfortran -O3 -fomit-frame-pointer -ffixed-form -fallow-argument-mismatch -c setuparraysize.f -o setuparraysize.o 
gfortran -O3 -fomit-frame-pointer -ffixed-form -fallow-argument-mismatch -c output1d.f -o output1d.o 
gfortran -O3 -fomit-frame-pointer -ffixed-form -fallow-argument-mismatch -c readguess1d.f -o readguess1d.o 
gfortran -O3 -fomit-frame-pointer -ffixed-form -fallow-argument-mismatch -c uvbind3d.f -o uvbind3d.o 
